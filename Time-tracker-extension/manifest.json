{
  "manifest_version": 3,
  "name": "Time Tracker",
  "version": "1.0",
  "permissions": [ "storage", "tabs" ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Time Tracker"
  },
  "host_permissions": [ "<all_urls>" ],
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "content_script.js" ],
      "run_at": "document_start"
    }
  ]
}
